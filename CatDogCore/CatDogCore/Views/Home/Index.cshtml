@{
    ViewData["Title"] = "Home Page";
    var images = ViewData["Images"] as string[];
}

<h1>CatDog-Core</h1>
<div class="row">
    <div class="container">
        <div class="row">
            @foreach (var item in images)
            {
                <div class="col-md-4">
                    <img src='@item' width=350 />
                </div>
            }
        </div>
    </div>
</div>