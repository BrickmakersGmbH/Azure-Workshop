@{
    var images = ViewData["Images"] as string[];
}
<div class="page-header">
    <div class="btn-toolbar pull-right">
        <div class="btn-group">
            <a class="btn btn-primary" asp-area="" asp-controller="Upload" asp-action="Index">Upload</a>
        </div>
    </div>
    <h2>CatDog-Core - @ViewData["Title"]</h2>
    <div class="row">
        <div class="col-md-4">
            <a class="btn btn-link" asp-area="" asp-controller="Home" asp-action="Index">Uploads</a>
            <a class="btn btn-link" asp-area="" asp-controller="Home" asp-action="Cats">Cats</a>
            <a class="btn btn-link" asp-area="" asp-controller="Home" asp-action="Dogs">Dogs</a>
            <a class="btn btn-link" asp-area="" asp-controller="Home" asp-action="Other">Other</a>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
    @foreach (var item in images)
    {
        <div class="col-md-4">
            <img src='@item' width=350/>
        </div>
    }
    </div>
</div>
